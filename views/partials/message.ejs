<% if (message.length > 0) { %>
<% message.forEach(msg => { %>
<div class="alert alert-danger alert-has-icon">
    <div class="alert-icon"><i class="fas fa-exclamation"></i></div>
    <div class="alert-body">
        <div class="alert-title">Failed</div>
        <%= msg %>
    </div>
</div>
<% }) %>
<% } %>